{
  "name": "@minimal-kit/starter-vite-ts",
  "author": "Minimals",
  "version": "6.0.1",
  "description": "Vite Starter & TypeScript",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite preview",
    "build": "tsc && vite build",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "fm:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "fm:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "rm:all": "rm -rf node_modules .next out dist build",
    "re:start": "yarn rm:all && yarn install && yarn dev",
    "re:build": "yarn rm:all && yarn install && yarn build",
    "re:build-npm": "npm run rm:all && npm install && npm run build",
    "compile": "graphql-codegen",
    "watch": "graphql-codegen -w"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "bash -c tsc --noEmit"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.11.2",
    "@emotion/cache": "^11.13.1",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@fontsource/barlow": "^5.0.13",
    "@fontsource/dm-sans": "^5.0.21",
    "@fontsource/inter": "^5.0.20",
    "@fontsource/nunito-sans": "^5.0.13",
    "@fontsource/public-sans": "^5.0.18",
    "@hookform/resolvers": "^3.9.0",
    "@iconify/react": "^5.0.2",
    "@mui/lab": "^5.0.0-alpha.173",
    "@mui/material": "^5.16.6",
    "@mui/x-data-grid": "^7.12.0",
    "@mui/x-date-pickers": "^7.12.0",
    "@mui/x-tree-view": "^7.12.0",
    "autosuggest-highlight": "^3.3.4",
    "axios": "^1.7.3",
    "dayjs": "^1.11.12",
    "framer-motion": "^11.3.21",
    "graphql": "^16.9.0",
    "history": "^5.3.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "qs": "^6.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.52.2",
    "react-imask": "^7.6.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-lazy-load-image-component": "^1.6.2",
    "react-organizational-chart": "^2.2.1",
    "react-phone-number-input": "^3.4.5",
    "react-router": "^6.26.0",
    "react-router-dom": "^6.26.0",
    "simplebar-react": "^3.2.6",
    "slugify": "^1.6.6",
    "sonner": "^1.5.0",
    "stylis": "^4.3.2",
    "stylis-plugin-rtl": "^2.1.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.3.3",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@parcel/watcher": "^2.4.1",
    "@types/autosuggest-highlight": "^3.2.3",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.1.0",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.15",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-infinite-scroll-component": "^5.0.0",
    "@types/react-lazy-load-image-component": "^1.6.4",
    "@types/stylis": "^4.2.6",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-perfectionist": "^3.1.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-unused-imports": "^3.2.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vite-plugin-checker": "^0.7.2"
  }
}
